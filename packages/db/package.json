{
  "name": "@ai-bookmark/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    }
  },
  "scripts": {
    "build": "dotenv -e ../../.env -- tsup src/index.ts src/schema.ts --format esm --dts",
    "dev": "dotenv -e ../../.env -- tsup src/index.ts src/schema.ts --format esm --dts --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "generate": "dotenv -e ../../.env -- drizzle-kit generate",
    "migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
    "push": "dotenv -e ../../.env -- drizzle-kit push",
    "studio": "dotenv -e ../../.env -- drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "^0.38.2",
    "postgres": "^3.4.5",
    "pgvector": "^0.2.0"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
